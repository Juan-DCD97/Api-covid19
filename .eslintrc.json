{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "space-before-function-paren": [
          "error",
          "always"
      ],
      "semi": [
          "warn",
          "never"
      ],
      "no-console": "off",
      "arrow-parens": ["error", "as-needed"],
      "import/no-unresolved": "off",
      "import/extensions": "off"
  }
}